{
    "name": "Cherry LA",
    "url": "https://cherryla.com",
    "loadJavascript": false,
    "catalogPageInformation": {
        "regex": [
            "/collections/shop(?!/products?)"
        ],
        "api": null
    },
    "productPageInformation": {
        "regex": [
            "/collections/shop/products/.+"
        ],
        "api": {
            "apiUrlEncoding": "{baseUrl}{route}{parameters}.js",
            "nameRoute": "title",
            "imageRoute": "images",
            "featuredImageRoute": "featured_image",
            "genderRoute": "tags",
            "clothingDescription": {
                "route": "description",
                "regex": null
            }
        }
    }
}