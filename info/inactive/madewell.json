{
    "name": "Madewell",
    "url": "https://www.madewell.com",
    "loadJavascript": true,
    "catalogPageInformation": {
        "regex": [
            "/(womens|mens)/.+",
            "(?:\\.com)(/[^\\?]+$)"
        ],
        "api": null
    },
    "productPageInformation": {
        "regex": [
            "(?:\\.com)(/.+\\.html\\?dwvar)"
        ],
        "identifiers": {
            "nameIdentifier": "product-name",
            "imageDivIdentifier": "product-images-desktop",
            "breadcrumbsIdentifier": "pdp-breadcrumbs",
            "clothingDescription": {
                "divIdentifier": "pdp-product-accordion",
                "regex": null
            }
        }
    }
}