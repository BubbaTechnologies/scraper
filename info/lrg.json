{
    "name": "LRG",
    "url": "https://l-r-g.com",
    "loadJavascript": true,
    "catalogPageInformation": {
        "regex": [
            "/collections/[a-z-]+$"
        ],
        "api": null
    },
    "productPageInformation": {
        "regex": [
            "collections/[a-z-]+/products/.+"
        ],
        "api": {
            "apiUrlEncoding": "{baseUrl}{route}.js",
            "nameRoute": "title",
            "imageRoute": "images",
            "genderRoute": "type",
            "clothingDescription": {
                "route": "description",
                "regex": null
            }
        }
    }
}